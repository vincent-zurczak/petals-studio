<plugin>

   <extension-point id="componentExtension" name="Extension to support component" schema="schema/componentExtension.exsd"/>

   <extension
         point="org.eclipse.ui.views">
      <view
            category="com.ebmwebsourcing.petals.common.views"
            class="com.ebmwebsourcing.petals.services.explorer.ServiceExplorerNavigator"
            icon="icons/obj16/petals_service.png"
            id="com.ebmwebsourcing.petals.services.explorer"
            name="Petals Services">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewerContentBinding
            viewerId="com.ebmwebsourcing.petals.services.explorer">
         <includes>
            <contentExtension
                  isRoot="true"
                  pattern="com.ebmwebsourcing.petals.services.endpointExplorerContent">
            </contentExtension>
            <contentExtension
                  pattern="com.ebmwebsourcing.petals.services.suEditorLinkHelper">
            </contentExtension>
         </includes>
      </viewerContentBinding>
      <dragAssistant
            class="com.ebmwebsourcing.petals.services.explorer.dnd.ServiceExplorerDragAdapterAssistant"
            viewerId="com.ebmwebsourcing.petals.services.explorer">
      </dragAssistant>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <navigatorContent
            activeByDefault="true"
            contentProvider="com.ebmwebsourcing.petals.services.explorer.ServiceExplorerContentProvider"
            id="com.ebmwebsourcing.petals.services.endpointExplorerContent"
            labelProvider="com.ebmwebsourcing.petals.services.explorer.ServiceExplorerLabelProvider"
            name="Petals services"
            priority="normal">
         <triggerPoints>
            <and>
               <instanceof
                     value="org.eclipse.core.resources.IProject">
               </instanceof>
               <test
                     property="projectNature"
                     value="com.ebmwebsourcing.petals.services.su.nature.SuNature">
               </test>
            </and>
         </triggerPoints>
         <actionProvider
               class="com.ebmwebsourcing.petals.services.explorer.ServiceExplorerActionProvider">
         </actionProvider>
         <commonSorter
               class="com.ebmwebsourcing.petals.services.explorer.CommonSorter"
               id="com.ebmwebsourcing.petals.services.explorer.CommonSorter">
         </commonSorter>
         <enablement>
            <instanceof
                  value="java.lang.Object">
            </instanceof>
         </enablement>
      </navigatorContent>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.linkHelper">
      <linkHelper
            class="com.ebmwebsourcing.petals.services.su.explorer.SuEditorLinkHelper"
            id="com.ebmwebsourcing.petals.services.suEditorLinkHelper">
         <editorInputEnablement></editorInputEnablement>
         <selectionEnablement></selectionEnablement>
      </linkHelper>
   </extension>
   <extension
         id="com.ebmwebsourcing.petals.tools.eclipse.su.main.nature"
         name="Petals Nature (Service Units)"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="com.ebmwebsourcing.petals.services.su.nature.SuNature">
         </run>
      </runtime>
      <builder
            id="com.ebmwebsourcing.petals.tools.eclipse.su.main.suBuilder">
      </builder>
   </extension>
   <extension
         id="com.ebmwebsourcing.petals.tools.eclipse.su.main.suBuilder"
         name="Petals Builder (Service Units)"
         point="org.eclipse.core.resources.builders">
      <builder
            callOnEmptyDelta="false"
            hasNature="true">
         <run
               class="com.ebmwebsourcing.petals.services.su.nature.SuBuilder">
         </run>
      </builder>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <navigatorContent
            activeByDefault="true"
            contentProvider="com.ebmwebsourcing.petals.services.su.projectscnf.SuProjectContentProvider"
            id="com.ebmwebsourcing.petals.projects.service-units"
            labelProvider="com.ebmwebsourcing.petals.common.internal.provisional.projectscnf.PetalsProjectLabelProvider"
            name="Service Units"
            priority="normal">
         <triggerPoints>
            <test
                  property="com.ebmwebsourcing.petals.common.project-category-id"
                  value="petals.category.service-units">
            </test>
         </triggerPoints>
         <dropAssistant
               class="org.eclipse.ui.navigator.resources.ResourceDropAdapterAssistant"
               id="com.ebmwebsourcing.petals.services.dropAssistant">
            <possibleDropTargets>
            	<or>
	                 <adapt type="org.eclipse.core.resources.IProject"/>
	                 <adapt type="org.eclipse.core.resources.IFolder"/>
	                 <adapt type="org.eclipse.core.resources.IFile"/>
              	</or>
            </possibleDropTargets>
         </dropAssistant>
         <commonSorter
               class="com.ebmwebsourcing.petals.common.internal.provisional.projectscnf.PetalsProjectsSorter"
               id="com.ebmwebsourcing.petals.common.projectscnf.PetalsProjectResourceSorter">
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            activeByDefault="true"
            contentProvider="com.ebmwebsourcing.petals.services.sa.projectscnf.SaProjectContentProvider"
            id="com.ebmwebsourcing.petals.projects.service-assemblies"
            labelProvider="com.ebmwebsourcing.petals.common.internal.provisional.projectscnf.PetalsProjectLabelProvider"
            name="Service Assemblies"
            priority="normal">
         <triggerPoints>
            <test
                  property="com.ebmwebsourcing.petals.common.project-category-id"
                  value="petals.category.service-assemblies">
            </test>
         </triggerPoints>
         <commonSorter
               class="com.ebmwebsourcing.petals.common.internal.provisional.projectscnf.PetalsProjectsSorter"
               id="com.ebmwebsourcing.petals.common.projectscnf.PetalsProjectResourceSorter">
         </commonSorter>
         <dropAssistant
               class="org.eclipse.ui.navigator.resources.ResourceDropAdapterAssistant"
               id="com.ebmwebsourcing.petals.services.dropAssistant">
            <possibleDropTargets>
               <or>
                  <adapt
                        type="org.eclipse.core.resources.IProject">
                  </adapt>
                  <adapt
                        type="org.eclipse.core.resources.IFolder">
                  </adapt>
                  <adapt
                        type="org.eclipse.core.resources.IFile">
                  </adapt>
               </or>
            </possibleDropTargets>
         </dropAssistant>
      </navigatorContent>
      <commonWizard
            type="import"
            wizardId="com.ebmwebsourcing.petals.services.importSaWizard">
         <enablement></enablement>
      </commonWizard>
      <commonFilter
            activeByDefault="true"
            class="com.ebmwebsourcing.petals.services.su.projectscnf.AttachedSuProjectsFilter"
            description="Service Units that are linked with Serivce Assembly projects"
            id="com.ebmwebsourcing.petals.services.orphanSuFilter"
            name="Attached Service Units">
      </commonFilter>
      <commonWizard
            type="export"
            wizardId="com.ebmwebsourcing.petals.services.suExportWizard">
         <enablement></enablement>
      </commonWizard>
      <commonWizard
            type="export"
            wizardId="com.ebmwebsourcing.petals.services.saExportWizard">
         <enablement></enablement>
      </commonWizard>
      <commonWizard
            type="export"
            wizardId="com.ebmwebsourcing.petals.services.suBulkExportWizard">
         <enablement></enablement>
      </commonWizard>
      <commonWizard
            type="import"
            wizardId="com.ebmwebsourcing.petals.services.importFileStructure">
         <enablement></enablement>
      </commonWizard>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewerContentBinding
            viewerId="com.ebmwebsourcing.petals.common.projects">
         <includes>
            <contentExtension
                  pattern="com.ebmwebsourcing.petals.services.orphanSuFilter">
            </contentExtension>
            <contentExtension
                  pattern="com.ebmwebsourcing.petals.services.editor.PetalsEditorsLinkHelper">
            </contentExtension></includes>
      </viewerContentBinding>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="com.ebmwebsourcing.petals.common.wizards"
            class="com.ebmwebsourcing.petals.services.sa.wizards.PetalsSaNewWizard"
            finalPerspective="com.ebmwebsourcing.petals.common.mainPerspective"
            hasPages="true"
            icon="icons/obj16/service_assembly.png"
            id="com.ebmwebsourcing.petals.services.sa.project"
            name="Service Assembly"
            project="true">
         <description>
            Create a Service Assembly project compatible with the Petals Maven plug-in
         </description>
      </wizard>
      <wizard
            category="com.ebmwebsourcing.petals.common.wizards"
            class="com.ebmwebsourcing.petals.services.su.wizards.NewServiceUnitSelectionWizard:provides"
            finalPerspective="com.ebmwebsourcing.petals.common.mainPerspective"
            hasPages="true"
            icon="icons/obj16/provides_3.0.png"
            id="com.ebmwebsourcing.petals.services.provider.project"
            name="Service Provider (SU)"
            project="true">
         <description>
            Create a Service Unit project to define provide a new Petals service inside the bus.
         </description>
      </wizard>
      <wizard
            category="com.ebmwebsourcing.petals.common.wizards"
            class="com.ebmwebsourcing.petals.services.su.wizards.NewServiceUnitSelectionWizard:consumes"
            finalPerspective="com.ebmwebsourcing.petals.common.mainPerspective"
            hasPages="true"
            icon="icons/obj16/consumes_3.0.png"
            id="com.ebmwebsourcing.petals.services.consumer.project"
            name="Service Consumer (SU)"
            project="true">
         <description>
            Create a Service Unit project to make a new Petals service available from outside of the bus.
         </description>
      </wizard>
   </extension>
   <extension
         id="com.ebmwebsourcing.petals.services.saNature"
         name="Petals Nature (Service Assemblies)"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="com.ebmwebsourcing.petals.services.sa.nature.SaNature">
         </run>
      </runtime>
      <builder
            id="com.ebmwebsourcing.petals.services.saBuilder">
      </builder>
   </extension>
